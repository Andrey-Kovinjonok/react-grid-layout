{
  "name": "react-styled-grid-layout",
  "version": "0.0.1",
  "description": "A draggable and resizable grid layout with responsive breakpoints, for React.",
  "main": "index.js",
  "scripts": {
    "dev-server-0": "rollup rollup-dev.config.js -c -w",
    "build": "webpack --env build",
    "build:rollup": "rollup rollup-build.config.js -c",
    "dev-server": "rollup -c -w",
    "build:old": "make build",
    "lint": "make lint",
    "test": "make test",
    "build-example": "make build-example",
    "view-example": "make view-example",
    "dev": "make dev",
    "prepublishOnly": "make build",
    "validate": "npm ls",
    "flow": "flow",
    "precommit": "lint-staged && make lint"
  },
  "repository": {
    "type": "git",
    " url": "git@github.com:Andrei-Kovinjonok/react-styled-grid-layout.git"
  },
  "keywords": [
    "react",
    "grid",
    "drag",
    "draggable",
    "resize",
    "resizable",
    "fluid",
    "responsive"
  ],
  "author": "Andrei Kovinjonok <noxerist@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/STRML/react-grid-layout/issues"
  },
  "homepage": "https://github.com/STRML/react-grid-layout",
  "dependencies": {
    "eslint-plugin-react-hooks": "^0.0.0",
    "lodash.isequal": "^4.5.0",
    "prop-types": "15.6.2",
    "react-draggable": "3.1.1",
    "react-resizable": "1.7.5",
    "react-use": "^5.1.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-hotreload": "^2.2.4",
    "rollup-plugin-replace": "^2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-flow-strip-types": "^7.2.3",
    "@babel/plugin-transform-react-jsx-source": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^22.2.2",
    "babel-loader": "^8.0.4",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-typecheck": "^3.9.0",
    "css-loader": "^0.28.9",
    "ejs": "^2.4.1",
    "eslint": "^5.10.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-react": "^7.11.1",
    "exports-loader": "^0.7.0",
    "flow-bin": "^0.89.0",
    "husky": "^1.2.0",
    "imports-loader": "^0.8.0",
    "jest-cli": "^23.6.0",
    "lint-staged": "^8.1.0",
    "lodash": "^4.17.11",
    "opener": "^1.5.1",
    "prettier": "^1.15.3",
    "react": "next",
    "react-dom": "next",
    "react-hot-loader": "^4.6.3",
    "rollup": "^0.68.2",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-size-snapshot": "^0.7.0",
    "style-loader": "^0.23.1",
    "valiquire": "^0.3.0",
    "webpack-cli": "^3.1.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "eslintConfig": {
    "env": {
      "node": 1,
      "browser": 1,
      "es6": 1,
      "jest/globals": true
    },
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "flowtype",
      "jsx-a11y",
      "jest"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:flowtype/recommended",
      "plugin:jsx-a11y/recommended"
    ]
  },
  "jest": {
    "testMatch": [
      "<rootDir>/test/spec/*.js"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "git add"
    ],
    "*.css": [
      "prettier --write",
      "git add"
    ]
  }
}
